import Html
import Html.Attributes
import List exposing (map)

main : Html.Html
main =
  let
    title = <h1>Hello</h1>
    name = "Homer"
    lis = map (\s -> <li>{=s}</li>) [ "Bart", "List", "Maggie" ]
  in
    <div class="container">
      {title}
      {=name} is the father of:
      <ul>{:lis}</ul>
    </div>
